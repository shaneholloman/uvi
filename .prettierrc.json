{
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 120,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "overrides": [
    {
      "files": ["*.yml", "*.yaml"],
      "options": {
        "parser": "yaml",
        "tabWidth": 2
      }
    },
    {
      "files": ["*.md"],
      "options": {
        "printWidth": 0,
        "proseWrap": "never"
      }
    }
  ]
}
